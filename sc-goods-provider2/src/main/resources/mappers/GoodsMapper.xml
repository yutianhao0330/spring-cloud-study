<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yutianhao.scgoodsprovider2.mapper.GoodsMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.yutianhao.sc.scgoodscommon.entity.Goods">
        <id column="id" property="id" />
        <result column="idNo" property="idNo" />
        <result column="name" property="name" />
        <result column="type" property="type" />
        <result column="norms" property="norms" />
        <result column="price" property="price" />
        <result column="num" property="num" />
        <result column="totalprice" property="totalprice" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, idNo, name, type, norms, price, num, totalprice
    </sql>

</mapper>
